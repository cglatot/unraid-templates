<?xml version="1.0"?>
<Container version="2">
  <Name>DAKOSYS</Name>
  <Repository>ghcr.io/cglatot/dakosys-overlay-only:latest</Repository>
  <Registry>https://github.com/cglatot/Dakosys-Overlay-Only/pkgs/container/dakosys-overlay-only</Registry>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Support>https://github.com/cglatot/Dakosys-Overlay-Only</Support>
  <Project>https://github.com/sahara101/Dakosys</Project>
  <Overview>DAKOSYS is a powerful tool for Plex users that creates and manages Trakt.tv lists and Kometa/PMM overlays. It helps you categorize anime episodes by type, track TV show statuses, display media file sizes, all running in Docker with automatic scheduling.</Overview>
  <Category>Tools:Utilities</Category>
  <WebUI/>
  <TemplateURL></TemplateURL>
  <Icon></Icon>
  <ExtraParams/>
  <PostArgs/>
  <DonateText></DonateText>
  <DonateLink></DonateLink>
  <Requires/>
  <Config Name="Config Directory" Target="/app" Default="/mnt/user/appdata/dakosys" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"/>
  <Config Name="Kometa Directory" Target="/kometa" Default="/mnt/user/appdata/Kometa" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"/>
  <TailscaleStateDir/>
</Container>
